<script type="text/javascript">
function deliciousCallback(posts) {
	var ul = document.createElement('ul')
	for (var i=0, post; post = posts[i]; i++) {
        var li = document.createElement('li')
        var a = document.createElement('a')
        var te = document.createElement('span')
        li.setAttribute('class', 'delicious-post')
        a.setAttribute('href', post.u)
        a.appendChild(document.createTextNode(post.d))
        li.appendChild(a)
		if(post.n) {
			li.appendChild(document.createTextNode(' '))
			te.appendChild(document.createTextNode(post.n))
			li.appendChild(te)
        }
        ul.appendChild(li)
	}
    document.getElementById('delicious-posts-tadpol').appendChild(ul)
}
</script>
<script type="text/javascript" src="http://feeds.delicious.com/v2/json/tadpol?count=3&amp;callback=deliciousCallback"></script>

