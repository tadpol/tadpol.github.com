<script language="javascript" type="text/javascript">
function readbookscallback(booklist, name) {
    var tds = $("table#readbooks td");
    var i = 0;
    for (var b in booklist.books ) {
        $(tds[i]).append("<a href=\"http://www.librarything.com/isbn/" + 
                         booklist.books[b].ISBN_cleaned + "\">" +
                         "<img src=\"" + booklist.books[b].cover + "\">" +
                         "<\/a>" +
                         "<a href=\"http://www.librarything.com/isbn/" + 
                         booklist.books[b].ISBN_cleaned + "\">" +
                         booklist.books[b].title + 
                         "<\/a> by " +
                         "<a href=\"http://www.librarything.com/author/" +
                         booklist.books[b].author_code + "\">" +
                         booklist.books[b].author_fl + "<\/a>"
                        );
        i++;
    }
}
</script>
