        $(event.target).next().show("normal");
    },function(event){
        $(event.target).next().hide("normal");
    });
    $("div#LinkFarm div.occontrol").append("<a>Show All<\/a> <a>Hide All<\/a>")
    $("div#LinkFarm div.occontrol a").contains("Show").click(function(){
        $("div#LinkFarm ul ul").show("normal");
    });
    $("div#LinkFarm div.occontrol a").contains("Hide").click(function(){
        $("div#LinkFarm ul ul").hide("normal");
    });
});
</script>
